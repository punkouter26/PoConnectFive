{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch PoConnectFive API",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/Po.ConnectFive.Api/bin/Debug/net10.0/Po.ConnectFive.Api.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Po.ConnectFive.Api",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "Now listening on: (https?://localhost:[0-9]+)",
                "uriFormat": "%s"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}